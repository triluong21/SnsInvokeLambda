{
  "name": "SnsInvokeLambda",
  "version": "1.0.0",
  "main": "starter.ts",
  "license": "MIT",
  "scripts": {
    "test": "node_modules/.bin/mocha \"./{,!(node_modules)/**/}*.spec.ts\" -t 10000 --require ts-node/register --require mocha.env --reporter spec",
    "watch": "node_modules/.bin/mocha \"./{,!(node_modules)/**/}*.spec.ts\" -t 10000 --require ts-node/register --require mocha.env --reporter spec --watch-extensions ts --watch",
    "lint": "node_modules/.bin/tslint src/**/*.ts",
    "build": "node_modules/.bin/webpack",
    "coverage": "node_modules/.bin/nyc node_modules/.bin/mocha \"./{,!(node_modules)/**/}*.spec.ts\" -t 10000 --require ts-node/register --require mocha.env --reporter spec",
    "coverageReport": "node_modules/.bin/nyc --reporter cobertura node_modules/.bin/mocha \"./{,!(node_modules)/**/}*.spec.ts\" -t 10000 --require ts-node/register --require mocha.env --reporter spec",
    "package": "node_modules/.bin/sls package --stage dev",
    "tsjson-get-response": "typescript-json-schema ./src/domain/OrderModel.ts IncomingPayload -o ./apiDocs/orderModel.json --ignoreErrors",
    "deploy-dev": "node_modules/.bin/sls deploy --stage dev",
    "deploy-qa": "node_modules/.bin/sls deploy --stage qa",
    "deploy-ba": "node_modules/.bin/sls deploy --stage ba",
    "deploy-prod": "node_modules/.bin/sls deploy --stage prod",
    "run-offline": "node_modules/.bin/sls offline start --stage dev",
    "install-dynamo": "node_modules/.bin/sls dynamodb install",
    "start-dynamo": "node_modules/.bin/sls dynamodb start",
    "validate": "aws cloudformation validate-template --template-body \"file://./.serverless/cloudformation-template-update-stack.json\" --region us-east-2",
    "serv-local": "node_modules/.bin/sls invoke  local -f SERV -p ./test/SERV/sampleEvent.json"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.19",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.21",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "serverless": "^1.37.1",
    "serverless-offline": "^4.4.0",
    "serverless-offline-sns": "^0.49.0",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}

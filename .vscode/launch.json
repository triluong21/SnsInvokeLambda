      "request": "launch",
      "name": "debug serverless offline",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless",
      "env": {
        "AWS_REGION": "us-east-2",
        "Region": "us-east-2",
        "IS_OFFLINE": "true",
